" Link for sync: https://raw.githubusercontent.com/timgreen/timgreenhomeconfig/master/modules/cVim/.cVimrc
" Settings

let barposition = "bottom"

" Sites
site 'https://www.wunderlist.com/*' {
  call :pintab
}

site 'http://www.pandora.com/*' {
  call :pintab
}

site 'https://inbox.google.com/*' {
  call :pintab
  unmap ?
  unmap e
  unmap j
  unmap k
}

site 'https://mail.google.com/*' {
  call :pintab
  unmap ?
  unmap e
  unmap j
  unmap k
}
