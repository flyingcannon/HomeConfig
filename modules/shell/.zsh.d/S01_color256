# vim: set ft=zsh foldmethod=marker autoindent expandtab shiftwidth=2 :

## 256 Colors Term
if [[ -n $COLORTERM ]]; then
  case $TERM in
    xterm)
    export TERM="xterm-256color"
    ;;
    screen)
    export TERM="screen-256color"
    ;;
    *)
    ;;
  esac
fi

## dircolor 256
eval $(dircolors ~/.dircolors-solarized/dircolors.256dark)
